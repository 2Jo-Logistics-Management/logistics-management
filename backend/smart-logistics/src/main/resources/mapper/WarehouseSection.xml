<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.douzon.smartlogistics.domain.warehousesection.dao.mapper.WarehouseSectionMapper">

    <update id="modify">
    update warehouseSection set sectionName = {sectionName}
    </update>
    <delete id="delete">
        Delete from warehouseSection where sectionName  = {sectionName}
    </delete>
    <select id="warehouseSectionList" resultType="com.douzon.smartlogistics.domain.entity.WarehouseSection">
        select
            sectionNAME
        from warehouseSection
    </select>

    <select id="searchWarehouseList" resultType="com.douzon.smartlogistics.domain.entity.WarehouseSection">
        select
            sectionName
        from warehouseSection where sectionName = {sectionName}
    </select>


</mapper>